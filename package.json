{
    "name": "vscode-openbrowser",
    "displayName": "OpenBrowser",
    "description": "open selection in browser",
    "version": "0.0.1",
    "publisher": "anykao",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "myplugin.openBrowser",
                "title": "Open Browser",
                "category": "My"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^10.9.4",
        "typescript": "^3.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "prettier": "^1.14.2"
    }
}
