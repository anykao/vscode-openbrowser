{
    "name": "vscode-openbrowser",
    "displayName": "OpenBrowser",
    "description": "open selection in browser",
    "version": "0.0.1",
    "publisher": "anykao",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openBrowser"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openBrowser",
                "title": "Open Browser",
                "category": "My"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^10.9.4",
        "typescript": "^3.0.3",
        "vscode": "^1.0.0"
    }
}
